#!/usr/bin/env python
# Sun Nov 23 16:20:12 EST 2008


import sys
import optparse

from rasmus import matrixlib



nrows, ncols, nnz, imat = matrixlib.iter_imat(sys.stdin)
#nrows, ncols, nnz, imat = matrixlib.transpose_imat(nrows, ncols, nnz, imat)
rmat = matrixlib.imat2rmat(nrows, ncols, nnz, imat)
matrixlib.write_rmat(sys.stdout, nrows, ncols, nnz, rmat)

